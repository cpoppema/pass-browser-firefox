{
    "author": "Cornelis Poppema",
    "manifest_version": 2,
    "icons": {
        "128": "images/icon-locked-128.png"
    },
    "browser_action": {
        "default_icon": "images/icon-locked-128.png",
        "default_popup": "html/popup.html"
    },
    "background": {
        "scripts": [
            "js/background.js"
        ]
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "js/content.js"
            ]
        }
    ],
    "permissions": [
        "<all_urls>",
        "activeTab",
        "clipboardWrite",
        "notifications",
        "storage"
    ],
    "web_accessible_resources": [
        "js/libs/jquery.js",
        "js/libs/progress.js",
        "js/content.js",
        "js/options.js",
        "js/popup.js",
        "html/options.html",
        "html/popup.html",
        "css/libs/bootstrap.css",
        "css/libs/progressjs.min.css",
        "css/popup.css",
        "images/icon-locked-128.png",
        "images/icon-unlocked-128.png"
    ],
    "content_security_policy": "script-src 'self'; object-src 'self'",
    "options_ui": {
        "page": "html/options.html",
        "chrome_style": false
    },
    "applications": {
      "gecko": {
        "id": "pass-browser@web-extensions.org"
      }
    }
}
